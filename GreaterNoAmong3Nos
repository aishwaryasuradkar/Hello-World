import java.util.Scanner;

public class Main{
    public static void main(String[] args){
      int a,b,c;
      Scanner sc=new Scanner(System.in);
      System.out.println("Enter 3 numbers:");
      a=sc.nextInt();
      b=sc.nextInt();
      c=sc.nextInt();
      if(a>b && a>c){
          System.out.println("a is greater amongst given two numbers");
          
      }
      else if(b>a && b>c){
          System.out.println("b is greater amongst given two numbers");
      }
      else{
          System.out.println("c is greater amongst given two numbers");
      }
    }
}
